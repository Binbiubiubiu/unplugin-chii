{
  "name": "unplugin-chii",
  "type": "module",
  "version": "0.1.13",
  "packageManager": "pnpm@7.1.1",
  "description": "Use chii for Vite and Webpack",
  "license": "MIT",
  "homepage": "https://github.com/Binbiubiubiu/unplugin-chii#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Binbiubiubiu/unplugin-chii.git"
  },
  "bugs": {
    "url": "https://github.com/Binbiubiubiu/unplugin-chii/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.js"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "require": "./dist/webpack.cjs",
      "import": "./dist/webpack.js"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "require": "./dist/nuxt.cjs",
      "import": "./dist/nuxt.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "require": "./dist/types.cjs",
      "import": "./dist/types.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint .",
    "play:vite": "pnpm --filter ./playgrounds/vite serve",
    "play:webpack": "pnpm --filter ./playgrounds/webpack serve",
    "play:chii": "chii start --port 8080",
    "prepublishOnly": "npm run lint && npm run build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "html-webpack-plugin": ">=3.2.x"
  },
  "dependencies": {
    "chii": "^1.8.0",
    "cross-spawn": "^7.0.3",
    "kolorist": "^1.7.0",
    "local-access": "^1.1.0",
    "lodash": "^4.17.21",
    "unplugin": "^1.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.2",
    "@types/cross-spawn": "^6.0.2",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.13.0",
    "@vitest/coverage-c8": "^0.28.4",
    "@webpack-cli/generators": "^3.0.1",
    "bumpp": "^8.2.1",
    "chalk": "^5.2.0",
    "cheerio": "1.0.0-rc.12",
    "eslint": "^8.33.0",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.12",
    "html-webpack-plugin": "^5.5.0",
    "node-fetch": "^3.3.0",
    "nodemon": "^2.0.20",
    "rimraf": "^4.1.2",
    "rollup": "^3.15.0",
    "terminate": "^2.6.1",
    "tsup": "^6.6.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vitest": "^0.28.4",
    "wait-port": "^1.0.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  }
}
